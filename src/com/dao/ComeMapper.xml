<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.ComeMapper">
<!-- 添加加入申请到数据库中 -->
	<insert id="addNewCome" parameterType="Come">
		insert into free_come (comeName,comeEmail,comePhone,introduce) 
			values (#{comeName},#{comeEmail},#{comePhone},#{introduce})
	</insert>

</mapper>