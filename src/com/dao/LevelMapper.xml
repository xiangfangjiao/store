<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.LevelMapper">
	<!-- 根据 levelMessage 查找 Level -->
	<select id="findLevelByMessage" resultType="Level">
		select * from free_level where levelMessage=#{levelMessage}
	</select>
</mapper>



